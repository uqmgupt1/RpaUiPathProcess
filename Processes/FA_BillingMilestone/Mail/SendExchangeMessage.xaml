                  </Assign.To>
                  <Assign.Value>
                    <InArgument x:TypeArguments="x:Int32">[timeout + 120000]</InArgument>
                  </Assign.Value>
                </Assign>
              </Sequence>
            </ActivityAction>
          </Catch>
        </TryCatch.Catches>
      </TryCatch>
    </While>
