  <Sequence sap2010:Annotation.AnnotationText="Description: Sends an email over SMTP protocol. All emails are sent from noreply@uq.edu.au.&#xA;&#xA;Inputs:&#xA;in_CredentialsAsset &lt;String&gt;.&#xA;in_SenderName &lt;String&gt;.&#xA;in_EmailRecipients &lt;String&gt;.&#xA;in_Subject &lt;String&gt;.&#xA;in_MessageBody &lt;String&gt;.&#xA;in_IsBodyHtml &lt;String&gt;.&#xA;in_Attachments &lt;String&gt;.&#xA;&#xA;Outputs: NA&#xA;&#xA;Pre Condition:&#xA;Server 'smtp.uq.edu.au' and port '587' is usable.&#xA;&#xA;Post Condition:&#xA;Throw error if no email recipients specified&#xA;Throw error if email not send after three tries" DisplayName="Send Email Message via SMTP" sap2010:WorkflowViewState.IdRef="Sequence_4">
    <Sequence.Variables>
      <Variable x:TypeArguments="x:Int32" Default="120000" Name="EMAIL_TIMEOUT" />
      <Variable x:TypeArguments="x:String" Name="username" />
      <Variable x:TypeArguments="x:String" Name="password" />
      <Variable x:TypeArguments="s:Exception" Default="[Nothing]" Name="mailError" />
      <Variable x:TypeArguments="x:Boolean" Default="True" Name="stillSending" />
    </Sequence.Variables>
    <If Condition="[in_EmailRecipients = &quot;;&quot;]" DisplayName="If no recipients" sap2010:WorkflowViewState.IdRef="If_1">
      <If.Then>
        <Throw DisplayName="Throw no recipients" Exception="[New System.Exception(&quot;No recipients to email report to.&quot;)]" sap2010:WorkflowViewState.IdRef="Throw_1" />
      </If.Then>
    </If>
    <If Condition="[in_CredentialsAsset IsNot Nothing]" DisplayName="If credentials specified" sap2010:WorkflowViewState.IdRef="If_4">
      <If.Then>
        <ui:InvokeWorkflowFile ContinueOnError="{x:Null}" DisplayName="Invoke GetAppCredentials workflow" sap2010:WorkflowViewState.IdRef="InvokeWorkflowFile_1" UnSafe="False" WorkflowFileName="Framework\GetAppCredentials.xaml">
          <ui:InvokeWorkflowFile.Arguments>
            <InArgument x:TypeArguments="x:String" x:Key="opt_Domain" />
            <InArgument x:TypeArguments="x:String" x:Key="in_Credential">[in_CredentialsAsset]</InArgument>
            <InArgument x:TypeArguments="x:String" x:Key="opt_Host">uq.edu.au</InArgument>
            <OutArgument x:TypeArguments="x:String" x:Key="out_Username">[username]</OutArgument>
            <OutArgument x:TypeArguments="ss:SecureString" x:Key="out_Password" />
            <OutArgument x:TypeArguments="x:String" x:Key="out_Plaintext">[password]</OutArgument>
          </ui:InvokeWorkflowFile.Arguments>
        </ui:InvokeWorkflowFile>
      </If.Then>
    </If>
    <While DisplayName="While sending hasn't reached max timespan" sap2010:WorkflowViewState.IdRef="While_1">
      <While.Variables>
        <Variable x:TypeArguments="x:Int32" Default="60000" Name="timeout" />
      </While.Variables>
      <While.Condition>[stillSending And timeout &lt; EMAIL_TIMEOUT]</While.Condition>
      <TryCatch DisplayName="Try Catch" sap2010:WorkflowViewState.IdRef="TryCatch_1">
        <TryCatch.Try>
          <Sequence sap2010:WorkflowViewState.IdRef="Sequence_1">
            <ui:LogMessage DisplayName="Log Message" sap2010:WorkflowViewState.IdRef="LogMessage_1" Level="Trace" Message="[&quot;Sending &quot; + in_Subject + &quot; to &quot; + in_EmailRecipients]" />
            <If Condition="[in_IsBodyHtml]" sap2010:WorkflowViewState.IdRef="If_2">
              <If.Then>
                <ui:SendMail Bcc="{x:Null}" Cc="{x:Null}" MailMessage="{x:Null}" TimeoutMS="{x:Null}" AttachmentsCollection="[in_Attachments]" Body="[in_MessageBody]" DisplayName="Send SMTP Mail Message" Email="[username]" EnableSSL="True" From="noreply@uq.edu.au" sap2010:WorkflowViewState.IdRef="SendMail_1" IsBodyHtml="True" Name="[in_SenderName]" Password="[password]" Port="587" SecureConnection="None" Server="smtp.uq.edu.au" Subject="[in_Subject]" To="[in_EmailRecipients]">
                  <ui:SendMail.Files>
                    <scg:List x:TypeArguments="InArgument(x:String)" Capacity="0" />
                  </ui:SendMail.Files>
                </ui:SendMail>
              </If.Then>
              <If.Else>
                <ui:SendMail Bcc="{x:Null}" Cc="{x:Null}" MailMessage="{x:Null}" TimeoutMS="{x:Null}" AttachmentsCollection="[in_Attachments]" Body="[in_MessageBody]" DisplayName="Send SMTP Mail Message" Email="[username]" EnableSSL="True" From="noreply@uq.edu.au" sap2010:WorkflowViewState.IdRef="SendMail_2" IsBodyHtml="False" Name="[in_SenderName]" Password="[password]" Port="587" SecureConnection="None" Server="smtp.uq.edu.au" Subject="[in_Subject]" To="[in_EmailRecipients]">
                  <ui:SendMail.Files>
                    <scg:List x:TypeArguments="InArgument(x:String)" Capacity="0" />
                  </ui:SendMail.Files>
                </ui:SendMail>
              </If.Else>
            </If>
            <Assign DisplayName="Assign sent" sap2010:WorkflowViewState.IdRef="Assign_1">
              <Assign.To>
                <OutArgument x:TypeArguments="x:Boolean">[stillSending]</OutArgument>
              </Assign.To>
              <Assign.Value>
                <InArgument x:TypeArguments="x:Boolean">False</InArgument>
              </Assign.Value>
            </Assign>
          </Sequence>
        </TryCatch.Try>
        <TryCatch.Catches>
          <Catch x:TypeArguments="s:Exception" sap2010:WorkflowViewState.IdRef="Catch`1_1">
            <ActivityAction x:TypeArguments="s:Exception">
              <ActivityAction.Argument>
                <DelegateInArgument x:TypeArguments="s:Exception" Name="exception" />
              </ActivityAction.Argument>
              <Sequence sap2010:WorkflowViewState.IdRef="Sequence_2">
                <If Condition="[mailError Is Nothing]" DisplayName="If mail error is not set" sap2010:WorkflowViewState.IdRef="If_5">
                  <If.Then>
                    <Assign DisplayName="Assign mail error" sap2010:WorkflowViewState.IdRef="Assign_5">
                      <Assign.To>
                        <OutArgument x:TypeArguments="s:Exception">[mailError]</OutArgument>
                      </Assign.To>
                      <Assign.Value>
                        <InArgument x:TypeArguments="s:Exception">[exception]</InArgument>
                      </Assign.Value>
                    </Assign>
                  </If.Then>
                </If>
                <ui:LogMessage DisplayName="Log Message" sap2010:WorkflowViewState.IdRef="LogMessage_2" Level="Error" Message="[mailError.Message]" />
                <Delay DisplayName="Delay 3s" Duration="00:00:03" sap2010:WorkflowViewState.IdRef="Delay_1" />
                <Assign DisplayName="Increment timeout" sap2010:WorkflowViewState.IdRef="Assign_2">
                  <Assign.To>
                    <OutArgument x:TypeArguments="x:Int32">[timeout]</OutArgument>
                  </Assign.To>
                  <Assign.Value>
                    <InArgument x:TypeArguments="x:Int32">[timeout + 120000]</InArgument>
                  </Assign.Value>
                </Assign>
              </Sequence>
            </ActivityAction>
          </Catch>
        </TryCatch.Catches>
      </TryCatch>
    </While>
    <If Condition="[stillSending]" DisplayName="If not sent after timeout" sap2010:WorkflowViewState.IdRef="If_3">
      <If.Then>
        <Throw DisplayName="Throw email error" Exception="[New ApplicationException(&quot;Email was not sent  to &quot; + in_EmailRecipients + &quot;after 3 tries&quot;, mailError)]" sap2010:WorkflowViewState.IdRef="Throw_2" />
      </If.Then>
    </If>
    <sads:DebugSymbol.Symbol></sads:DebugSymbol.Symbol>
  </Sequence>
