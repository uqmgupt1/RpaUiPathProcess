              <Assign.Value>
                <InArgument x:TypeArguments="x:String">[out_Username + "@" + in_Host]</InArgument>
              </Assign.Value>
            </Assign>
          </If.Then>
        </If>
      </Sequence>
    </TryCatch.Finally>
