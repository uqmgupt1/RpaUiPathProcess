  <Sequence DisplayName="SanitiseHtmlText" sap2010:WorkflowViewState.IdRef="Sequence_2">
    <While DisplayName="While more lines" sap2010:WorkflowViewState.IdRef="While_5">
      <While.Variables>
        <Variable x:TypeArguments="x:Int32" Default="0" Name="i" />
      </While.Variables>
      <While.Condition>[i &lt; io_Text.Length]</While.Condition>
      <Sequence sap2010:WorkflowViewState.IdRef="Sequence_5">
        <While DisplayName="While line contains escaped content or tags" sap2010:WorkflowViewState.IdRef="While_1" Condition="[Regex.IsMatch(io_Text(i), &quot;&amp;[a-z0-9#]+;&quot;) Or Regex.IsMatch(io_Text(i), &quot;&lt;\/?\w+( [^&gt;]+)?&gt;&quot;)]">
          <Sequence sap2010:WorkflowViewState.IdRef="Sequence_1">
            <Assign DisplayName="Assign unescape html tags, replace nbsp with space" sap2010:WorkflowViewState.IdRef="Assign_1">
              <Assign.To>
                <OutArgument x:TypeArguments="x:String">[io_Text(i)]</OutArgument>
              </Assign.To>
              <Assign.Value>
                <InArgument x:TypeArguments="x:String">[WebUtility.HtmlDecode(io_Text(i)).Replace(" ", " ")]</InArgument>
              </Assign.Value>
            </Assign>
            <Assign DisplayName="Assign replaced line breaks" sap2010:WorkflowViewState.IdRef="Assign_2">
              <Assign.To>
                <OutArgument x:TypeArguments="x:String">[io_Text(i)]</OutArgument>
              </Assign.To>
              <Assign.Value>
                <InArgument x:TypeArguments="x:String">[Regex.Replace(io_Text(i), "&lt;br\s*\/&gt;", vbNewLine)]</InArgument>
              </Assign.Value>
            </Assign>
            <Assign DisplayName="Assign replaced url links" sap2010:WorkflowViewState.IdRef="Assign_11">
              <Assign.To>
                <OutArgument x:TypeArguments="x:String">[io_Text(i)]</OutArgument>
              </Assign.To>
              <Assign.Value>
                <InArgument x:TypeArguments="x:String">[Regex.Replace(io_Text(i), " ?&lt;a href=""([^""]+)""[^&gt;]+&gt; ?", " $1 ")]</InArgument>
              </Assign.Value>
            </Assign>
            <Assign DisplayName="Assign tagless text" sap2010:WorkflowViewState.IdRef="Assign_3">
              <Assign.To>
                <OutArgument x:TypeArguments="x:String">[io_Text(i)]</OutArgument>
              </Assign.To>
              <Assign.Value>
                <InArgument x:TypeArguments="x:String">[Regex.Replace(io_Text(i), "&lt;\/?\w+( [^&gt;]+)?&gt;", "")]</InArgument>
              </Assign.Value>
            </Assign>
          </Sequence>
        </While>
        <Assign DisplayName="Increment i" sap2010:WorkflowViewState.IdRef="Assign_8">
          <Assign.To>
            <OutArgument x:TypeArguments="x:Int32">[i]</OutArgument>
          </Assign.To>
          <Assign.Value>
            <InArgument x:TypeArguments="x:Int32">[i + 1]</InArgument>
          </Assign.Value>
        </Assign>
      </Sequence>
    </While>
    <While DisplayName="While trimming leading whitespace" sap2010:WorkflowViewState.IdRef="While_3" Condition="[io_Text.Length &gt; 0 AndAlso String.IsNullOrWhiteSpace(io_Text.First)]">
      <Assign DisplayName="Assign text minus first" sap2010:WorkflowViewState.IdRef="Assign_5">
        <Assign.To>
          <OutArgument x:TypeArguments="s:String[]">[io_Text]</OutArgument>
        </Assign.To>
        <Assign.Value>
          <InArgument x:TypeArguments="s:String[]">[io_Text.Skip(1).ToArray]</InArgument>
        </Assign.Value>
      </Assign>
    </While>
    <While DisplayName="While trimming tailing whitespace" sap2010:WorkflowViewState.IdRef="While_4" Condition="[io_Text.Length &gt; 0 AndAlso String.IsNullOrWhiteSpace(io_Text.Last)]">
      <Assign DisplayName="Assign text minus last" sap2010:WorkflowViewState.IdRef="Assign_6">
        <Assign.To>
          <OutArgument x:TypeArguments="s:String[]">[io_Text]</OutArgument>
        </Assign.To>
        <Assign.Value>
          <InArgument x:TypeArguments="s:String[]">[io_Text.Take(io_Text.Length - 1).ToArray]</InArgument>
        </Assign.Value>
      </Assign>
    </While>
    <Assign DisplayName="Assign resplit lines" sap2010:WorkflowViewState.IdRef="Assign_10">
      <Assign.To>
        <OutArgument x:TypeArguments="s:String[]">[io_Text]</OutArgument>
      </Assign.To>
      <Assign.Value>
        <InArgument x:TypeArguments="s:String[]">[String.Join(vbNewLine, io_Text).Split(vbNewLine.ToCharArray, StringSplitOptions.RemoveEmptyEntries)]</InArgument>
      </Assign.Value>
    </Assign>
