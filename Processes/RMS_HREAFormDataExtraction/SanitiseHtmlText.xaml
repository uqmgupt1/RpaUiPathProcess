    <ui:ForEach x:TypeArguments="x:String" CurrentIndex="{x:Null}" DisplayName="For Each line" sap2010:WorkflowViewState.IdRef="ForEach`1_1" Values="[io_Text.Split(vbNewLine.ToCharArray, StringSplitOptions.RemoveEmptyEntries)]">
