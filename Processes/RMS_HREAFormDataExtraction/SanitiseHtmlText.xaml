  <Sequence DisplayName="SanitiseHtmlText" sap2010:WorkflowViewState.IdRef="Sequence_2">
    <While DisplayName="While line contains escaped content or tags" sap2010:WorkflowViewState.IdRef="While_1" Condition="[Regex.IsMatch(io_Text, &quot;&amp;[a-z0-9#]+;&quot;) Or Regex.IsMatch(io_Text, &quot;&lt;\/?\w+( [^&gt;]+)?&gt;&quot;)]">
      <Sequence sap2010:WorkflowViewState.IdRef="Sequence_1">
        <Assign DisplayName="Assign unescape html tags, replace nbsp with space" sap2010:WorkflowViewState.IdRef="Assign_1">
          <Assign.To>
            <OutArgument x:TypeArguments="x:String">[io_Text]</OutArgument>
          </Assign.To>
          <Assign.Value>
            <InArgument x:TypeArguments="x:String">[WebUtility.HtmlDecode(io_Text).Replace(" ", " ")]</InArgument>
          </Assign.Value>
        </Assign>
        <Assign DisplayName="Assign replaced line breaks" sap2010:WorkflowViewState.IdRef="Assign_2">
          <Assign.To>
            <OutArgument x:TypeArguments="x:String">[io_Text]</OutArgument>
          </Assign.To>
          <Assign.Value>
            <InArgument x:TypeArguments="x:String">[Regex.Replace(io_Text, "&lt;br\s*\/&gt;", vbNewLine)]</InArgument>
          </Assign.Value>
        </Assign>
        <Assign DisplayName="Assign replaced compound spaces" sap2010:WorkflowViewState.IdRef="Assign_14">
          <Assign.To>
            <OutArgument x:TypeArguments="x:String">[io_Text]</OutArgument>
          </Assign.To>
          <Assign.Value>
            <InArgument x:TypeArguments="x:String" xml:space="preserve">[Regex.Replace(io_Text, "  +", " ")]</InArgument>
          </Assign.Value>
        </Assign>
        <Assign DisplayName="Assign replaced url links" sap2010:WorkflowViewState.IdRef="Assign_11">
          <Assign.To>
            <OutArgument x:TypeArguments="x:String">[io_Text]</OutArgument>
          </Assign.To>
          <Assign.Value>
            <InArgument x:TypeArguments="x:String">[Regex.Replace(io_Text, " ?&lt;a href=""([^""]+)""[^&gt;]+&gt; ?", " $1 ")]</InArgument>
          </Assign.Value>
        </Assign>
        <Assign DisplayName="Assign tagless text" sap2010:WorkflowViewState.IdRef="Assign_3">
          <Assign.To>
            <OutArgument x:TypeArguments="x:String">[io_Text]</OutArgument>
          </Assign.To>
          <Assign.Value>
            <InArgument x:TypeArguments="x:String">[Regex.Replace(io_Text, "&lt;\/?\w+( [^&gt;]+)?&gt;", "")]</InArgument>
          </Assign.Value>
        </Assign>
      </Sequence>
    </While>
    <While DisplayName="While trimming leading whitespace" sap2010:WorkflowViewState.IdRef="While_3" Condition="[io_Text.Length &gt; 0 AndAlso String.IsNullOrWhiteSpace(io_Text.First)]">
      <Assign DisplayName="Assign text minus first" sap2010:WorkflowViewState.IdRef="Assign_5">
        <Assign.To>
          <OutArgument x:TypeArguments="x:String">[io_Text]</OutArgument>
        </Assign.To>
        <Assign.Value>
          <InArgument x:TypeArguments="x:String">[io_Text.Skip(1).ToArray]</InArgument>
        </Assign.Value>
      </Assign>
    </While>
    <While DisplayName="While trimming tailing whitespace" sap2010:WorkflowViewState.IdRef="While_4" Condition="[io_Text.Length &gt; 0 AndAlso String.IsNullOrWhiteSpace(io_Text.Last)]">
      <Assign DisplayName="Assign text minus last" sap2010:WorkflowViewState.IdRef="Assign_6">
        <Assign.To>
          <OutArgument x:TypeArguments="x:String">[io_Text]</OutArgument>
        </Assign.To>
        <Assign.Value>
          <InArgument x:TypeArguments="x:String">[io_Text.Take(io_Text.Length - 1).ToArray]</InArgument>
        </Assign.Value>
      </Assign>
    </While>
    <Assign DisplayName="Assign realigned text" sap2010:WorkflowViewState.IdRef="Assign_13">
      <Assign.To>
        <OutArgument x:TypeArguments="x:String">[io_Text]</OutArgument>
      </Assign.To>
      <Assign.Value>
        <InArgument x:TypeArguments="x:String">[String.Join(vbNewLine, io_Text.Split(vbNewLine.ToCharArray, StringSplitOptions.RemoveEmptyEntries)).Trim]</InArgument>
      </Assign.Value>
    </Assign>
    <ui:CommentOut DisplayName="Comment Out" sap2010:WorkflowViewState.IdRef="CommentOut_1">
      <ui:CommentOut.Body>
        <Sequence DisplayName="Ignored Activities" sap2010:WorkflowViewState.IdRef="Sequence_3">
          <Assign sap2010:WorkflowViewState.IdRef="Assign_12">
            <Assign.To>
              <OutArgument x:TypeArguments="x:String">[io_Text]</OutArgument>
            </Assign.To>
            <Assign.Value>
              <InArgument x:TypeArguments="x:String">[System.Text.Encoding.ASCII.GetString(System.Text.Encoding.UTF8.GetBytes(io_Text)).Trim]</InArgument>
            </Assign.Value>
