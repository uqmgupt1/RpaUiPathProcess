        <OutArgument x:TypeArguments="x:String">[messageText]</OutArgument>
      </Assign.To>
      <Assign.Value>
        <InArgument x:TypeArguments="x:String" xml:space="preserve">["The following task was processed by FTAE bot.  The status is """ + in_Status.ToUpper + """.  The details are listed below." + "&lt;br&gt;" + "&lt;br&gt;" + "Case ID: " + in_TransactionItem.Reference + "&lt;br&gt;" + "Employee Name: " + in_EmployeeName + "&lt;br&gt;" + "Employee Number: " + in_EmployeeNumber + "&lt;br&gt;" + "Current End Date: " + in_CurrentEndDate.ToString("dd-MMM-yyyy") + "&lt;br&gt;" + "Proposed Start Date: " + in_ProposedStartDate.ToString("dd-MMM-yyyy") + "&lt;br&gt;" + "Proposed End Date: " + in_ProposedEndDate.ToString("dd-MMM-yyyy") + "&lt;br&gt;" + "PST Name: " + in_pstName + "&lt;br&gt;" + "Status: " + in_Status + "&lt;br&gt;" + "Notes: " + in_Notes]</InArgument>
