                </If.Then>
                <If.Else>
                  <Sequence sap2010:WorkflowViewState.IdRef="Sequence_3">
                    <Assign DisplayName="Assign operatingEnvironment = &quot;&quot;" sap2010:WorkflowViewState.IdRef="Assign_8">
                      <Assign.To>
