              <ui:LogMessage DisplayName="Log Message" sap2010:WorkflowViewState.IdRef="LogMessage_7" Level="Error" Message="[&quot;Error message: '&quot; + exception.Message + &quot;' source: '&quot; + exception.Source + &quot;'&quot;]" />
