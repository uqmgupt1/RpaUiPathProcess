    <ui:InvokeCode ContinueOnError="{x:Null}" Code="If (student_dataRow.Item(&quot;EMPLID&quot;).ToString.Trim.Length &lt; 1) Then&#xD;&#xA;	errorMsg=&quot;Value Missing - EMPLID&quot;&#xD;&#xA;End If&#xD;&#xA;&#xD;&#xA;If (student_dataRow.Item(&quot;FAMILY_NAME&quot;).ToString.Trim.Length &lt; 1) And errorMsg Is Nothing Then&#xD;&#xA;	errorMsg=&quot;Value Missing - FAMILY NAME&quot;&#xD;&#xA;End If &#xD;&#xA;&#xD;&#xA;If (student_dataRow.Item(&quot;BIRTH_DATE&quot;).ToString.Trim.Length &lt; 1) And errorMsg Is Nothing Then&#xD;&#xA;	errorMsg=&quot;Value Missing - BIRTH DATE&quot;&#xD;&#xA;End If&#xD;&#xA;&#xD;&#xA;If (student_dataRow.Item(&quot;PASSPORT_NUMBER&quot;).ToString.Trim.Length &lt; 1) And errorMsg Is Nothing Then&#xD;&#xA;	errorMsg=&quot;Value Missing - PASSPORT NUMBER&quot;&#xD;&#xA;End If&#xD;&#xA;&#xD;&#xA;If (student_dataRow.Item(&quot;PASSPORT_COUNTRY&quot;).ToString.Trim.Length &lt; 1) And errorMsg Is Nothing Then&#xD;&#xA;	errorMsg=&quot;Value Missing - PASSPORT COUNTRY&quot;&#xD;&#xA;End If&#xD;&#xA;" DisplayName="Invoke Code" sap2010:WorkflowViewState.IdRef="InvokeCode_1">
